{
    "name": "lase-peco/localization",
    "description": "A simple localization library",
    "keywords": [
        "lase-peco",
        "localization"
    ],
    "homepage": "https://github.com/lase-peco/localization",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Ahmed Dabak",
            "email": "a.dabak@lase-peco.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.1.0",
        "laravel/framework": "~5.2.0||~5.3.0||~5.4.0||~5.5.0||~5.6.0||~5.7.0||~5.8.0||^6.0||^7.0||^8.0||^9.0",
        "ext-intl": "*"
    },
    "require-dev": {
        "orchestra/testbench": "^4.0",
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "LasePeco\\Localization\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LasePeco\\Localization\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "LasePeco\\Localization\\LocalizationServiceProvider"
            ],
            "aliases": {
                "Localization": "Localization"
            }
        }
    }
}
